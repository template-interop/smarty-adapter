    "description": "Smarty adapter for template-interop/engine",
    "keywords": [
        "render",
        "template",
        "engine",
        "templating",
        "output",
        "smarty"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Keven",
            "email": "hello@keven.fr"
        }
    ],
    "require": {
        "php": ">=7.1",
        "template-interop/engine": "^0.0.2",
        "smarty/smarty": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Interop\\Template\\Smarty\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Interop\\Template\\Smarty\\": "src/"
        }
    }
